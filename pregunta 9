#include <iostream>
#include <string>
using namespace std;
void leerConjunto(string& conjunto, int n) {
    cout << "Ingrese " << n << " caracteres para el conjunto:" << endl;
    conjunto.clear();
    for (int i = 0; i < n; ++i) {
        char c;
        cin >> c;
        // Evitar duplicados en el conjunto
        if (conjunto.find(c) == string::npos) {
            conjunto += c;
        }
    }
}
string interseccionConjuntos(const string& A, const string& B) {
    string C;
    for (char c : A) {
        if (B.find(c) != string::npos) {
            C += c;
        }
    }
    return C;
}
void mostrarConjunto(const string& conjunto) {
    cout << "Conjunto resultante (interseccion): ";
    for (char c : conjunto) {
        cout << c << ' ';
    }
    cout << endl;
}
int main() {
    int nA, nB;
    cout << "Ingrese la cantidad de caracteres para el conjunto A: ";
    cin >> nA;
    string A;
    leerConjunto(A, nA);

    cout << "Ingrese la cantidad de caracteres para el conjunto B: ";
    cin >> nB;
    string B;
    leerConjunto(B, nB);

    string C = interseccionConjuntos(A, B);
    mostrarConjunto(C);

    return 0;
}
