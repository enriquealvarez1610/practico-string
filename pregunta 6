#include <iostream>
#include <string>
using namespace std;
string eliminarVocales(const string& texto) {
    string resultado;
    for (char c : texto) {
        if (c != 'a' && c != 'e' && c != 'i' && c != 'o' && c != 'u' &&
            c != 'A' && c != 'E' && c != 'I' && c != 'O' && c != 'U') {
            resultado += c;
        }
    }
    return resultado;
}
int main() {
    string texto;
    cout << "Ingrese una cadena de texto: ";
    getline(cin, texto);

    string textoSinVocales = eliminarVocales(texto);
    cout << "Cadena sin vocales: " << textoSinVocales << endl;

    return 0;
}
